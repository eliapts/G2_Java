import java.io.File;
import java.io.FileWriter;
import java.io.IOException;
import java.util.Random;
import java.util.Scanner;
import java.io.FileReader;
import java.io.*;


public class Main {
  int a = -48;
  int b = 12 ; 

  public void method1() throws IOException {
      Scanner input = new Scanner(System.in);
      System.out.println("Sheiyvanet n : ");
      int n = input.nextInt();
      if (n>10) {
        Random rand = new Random();
        for (int i = 0; i<n ; i++){
          int x = rand.nextInt(b-a+1)+a;
          FileWriter filew = new FileWriter(new File("numbers.txt"),true);
          filew.write(x + "\n");
          filew.close();}}
      else {
        throw new ArithmeticException("შეიყვანეთ 10ზე მეტი რიცხვი!!!");
        
      }
        
      }
      public void method2() throws IOException{
             BufferedReader rdBf = new BufferedReader(new FileReader("numbers.txt"));
           
           String st = null;
           int mult=1;
           while ((st = rdBf.readLine()) != null){
              int nums=Integer.parseInt(st);
              if (nums<0){
                  mult*=nums;
              }
        }
           System.out.println(mult);
           rdBf.close();


 }
 
  public static void main(String[] args) throws IOException {
    myclass obj = new myclass();
    obj.method1();
    obj.method2();
  }

}
